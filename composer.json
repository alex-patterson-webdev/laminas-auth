{
  "name": "alex-patterson-webdev/event-dispatcher",
  "version": "1.0.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "laminas/laminas-authentication": "^2.7",
    "laminas/laminas-session": "^2.9",
    "alex-patterson-webdev/event-dispatcher": "dev-master",
    "alex-patterson-webdev/laminas-factory": "dev-master"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "Arp\\Auth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\Auth\\": "test/phpunit/"
    }
  },
  "scripts": {
    "test": "phpunit --coverage-clover=coverage.xml"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
